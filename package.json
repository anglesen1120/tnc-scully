{
  "name": "tnc-scully",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod && scully --nw --scanRoutes && npm run pwa:regen",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "scully",
    "scully:serve": "scully serve",
    "pwa:regen": "rm dist/static/ngsw.json;node_modules/.bin/ngsw-config dist/static ./ngsw-config.json"
  },
  "private": true,
  "prettier": {
    "tabWidth": 2,
    "trailingComma": "all",
    "semi": true,
    "singleQuote": true,
    "printWidth": 100
  },
  "dependencies": {
    "@angular/animations": "11.0.2",
    "@angular/common": "11.0.2",
    "@angular/compiler": "11.0.2",
    "@angular/core": "11.0.2",
    "@angular/forms": "11.0.2",
    "@angular/platform-browser": "11.0.2",
    "@angular/platform-browser-dynamic": "11.0.2",
    "@angular/router": "11.0.2",
    "@angular/service-worker": "11.0.2",
    "@scullyio/init": "1.0.1",
    "@scullyio/ng-lib": "1.0.0",
    "@scullyio/scully": "1.0.3",
    "rxjs": "6.6.3",
    "tslib": "2.0.3",
    "zone.js": "0.11.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.1",
    "@angular-devkit/build-angular": "0.1100.2",
    "@angular/cli": "11.0.2",
    "@angular/compiler-cli": "11.0.2",
    "@fullhuman/postcss-purgecss": "3.0.0",
    "@gammastream/scully-plugin-http404": "1.0.4",
    "@nartc/scully-plugin-google-gtag": "0.0.8",
    "@types/gtag.js": "0.0.3",
    "@types/node": "14.11.10",
    "postcss-import": "12.0.1",
    "postcss-loader": "4.0.4",
    "postcss-scss": "3.0.2",
    "prettier": "2.1.2",
    "scully-plugin-minify-html": "4.0.0",
    "tailwindcss": "1.9.5",
    "ts-node": "9.0.0",
    "tslint": "~6.1.3",
    "typescript": "4.0.3"
  }
}
