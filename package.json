{
  "name": "tnc-scully",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod && scully --nw --scanRoutes && npm run pwa:regen",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "scully",
    "scully:serve": "scully serve",
    "pwa:regen": "rm dist/static/ngsw.json;node_modules/.bin/ngsw-config dist/static ./ngsw-config.json"
  },
  "private": true,
  "prettier": {
    "tabWidth": 2,
    "trailingComma": "all",
    "semi": true,
    "singleQuote": true,
    "printWidth": 100
  },
  "dependencies": {
    "@angular/animations": "10.0.14",
    "@angular/common": "10.0.14",
    "@angular/compiler": "10.0.14",
    "@angular/core": "10.0.14",
    "@angular/forms": "10.0.14",
    "@angular/platform-browser": "10.0.14",
    "@angular/platform-browser-dynamic": "10.0.14",
    "@angular/router": "10.0.14",
    "@angular/service-worker": "10.0.14",
    "@scullyio/init": "1.0.0-beta.3",
    "@scullyio/ng-lib": "1.0.0-beta.2",
    "@scullyio/scully": "1.0.0-beta.3",
    "rxjs": "6.6.2",
    "tslib": "2.0.1",
    "zone.js": "0.11.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.0",
    "@angular-devkit/build-angular": "0.1000.8",
    "@angular/cli": "10.0.8",
    "@angular/compiler-cli": "10.0.14",
    "@fullhuman/postcss-purgecss": "2.3.0",
    "@gammastream/scully-plugin-http404": "1.0.3",
    "@nartc/scully-plugin-google-gtag": "0.0.8",
    "@types/gtag.js": "0.0.3",
    "@types/node": "14.6.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-scss": "2.1.1",
    "prettier": "2.1.1",
    "scully-plugin-minify-html": "3.2.0",
    "tailwindcss": "1.7.5",
    "ts-node": "9.0.0",
    "tslint": "~6.1.3",
    "typescript": "3.9.7"
  }
}
